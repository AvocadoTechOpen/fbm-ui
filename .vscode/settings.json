{
  "editor.fontSize": 18,
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true
  },
  "eslint.options": { 
    "extensions": [
      ".js",
      ".ts",
      ".tsx"
    ]
  },
  "emmet.includeLanguages": {
    "wxml": "html"
  },
  "editor.detectIndentation": false,
  "editor.tabSize": 2,
}